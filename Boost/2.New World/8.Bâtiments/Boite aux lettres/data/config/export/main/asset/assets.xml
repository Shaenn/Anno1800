<ModOps>
  <ModOp Type="merge" GUID="3661" Path="/Values/FactoryBase">
    <FactoryBase>
      <FactoryOutputs>
        <Item>
          <Product>535</Product>
          <!--Local Mail-->
          <Amount>100</Amount>
          <StorageAmount>8000</StorageAmount>
        </Item>
      </FactoryOutputs>
      <CycleTime>15</CycleTime>
      <WarehouseTransporterAsset>829</WarehouseTransporterAsset>
      <!--MailTransporter-->
      <MissingConnectionInfolayer>0</MissingConnectionInfolayer>
      <ClampMaxTransporterLogisticCost>1</ClampMaxTransporterLogisticCost>
    </FactoryBase>
  </ModOp>

  <ModOp Type="merge" GUID="3661" Path="/Values/ItemContainer">
    <ItemContainer>
      <SocketScopeRadius>6666</SocketScopeRadius>
    </ItemContainer>
  </ModOp>

  <ModOp Type="merge" GUID="3661" Path="/Values/Warehouse">
    <Warehouse>
      <WarehouseStorage>
        <StorageMax>8000</StorageMax>
      </WarehouseStorage>
    </Warehouse>
  </ModOp>

  <!-- AmÃ©lioration du personnage pour produire plus de rapport -->
  <ModOp Path="/Values/FactoryUpgrade"
         GUID="4270"
         Type="merge">
    <FactoryUpgrade>
      <AdditionalOutput>
        <Item>
          <Product>4267</Product>
          <AdditionalOutputCycle>1</AdditionalOutputCycle>
          <Amount>100</Amount>
        </Item>
      </AdditionalOutput>
    </FactoryUpgrade>
  </ModOp>
</ModOps>
