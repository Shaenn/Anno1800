<ModOps>
  <ModOp Path="/Values/FactoryBase"
         GUID="5457"
         Type="merge">
    <FactoryBase>
      <FactoryOutputs>
        <Item>
          <Amount>100</Amount>
          <StorageAmount>8000</StorageAmount>
        </Item>
      </FactoryOutputs>
      <CycleTime>15</CycleTime>
    </FactoryBase>
  </ModOp>

  <!-- Réduction du nombre de module requis -->
  <ModOp Path="/Values/ModuleOwner" GUID="5457" Type="merge">
    <ModuleLimits>
      <Main>
        <Limit>
          1
        </Limit>
      </Main>
    </ModuleLimits>
  </ModOp>

  <!-- Boost de production sur le pâturage électrifié -->
  <ModOp Path="/Values/FactoryBase" GUID="8026" Type="merge">
    <FactoryBase>
      <FactoryOutputs>
        <Item>
          <Product>5401</Product>
          <Amount>100</Amount>
          <StorageAmount>8000</StorageAmount>
        </Item>
      </FactoryOutputs>
      <CycleTime>15</CycleTime>
      <WarehouseTransporterAsset>0</WarehouseTransporterAsset>
      <OutputAmountFactor>0</OutputAmountFactor>
      <ClampedMaxTransporterLogisticCost>0.1</ClampedMaxTransporterLogisticCost>
    </FactoryBase>
  </ModOp>
</ModOps>
